package lista_circular;

@SuppressWarnings("rawtypes")
public class colas<T> extends listac {

	listac<T> cola;
	
	@SuppressWarnings({ "unchecked" })
	public colas(){
		cola = new listac();
	}
	
	public int size(){
		return cola.tamanio();
	}
	
	public void push(T obj){				//añadir
		cola.addLast(obj);
	}
	
	public T pop(){							//obtener
		return cola.borrar();
	}
	
	public boolean colaVacia(){
		boolean res = false;
		
		if(cola != null)
			res = true;
			
		return res;
	}
}
