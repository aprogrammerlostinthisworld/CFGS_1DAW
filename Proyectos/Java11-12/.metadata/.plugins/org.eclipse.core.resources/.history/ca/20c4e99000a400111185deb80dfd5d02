public class pilad {

	private class CElemento{
		private char dato;
		private CElemento siguiente;
		
		public CElemento(){}
		
		public CElemento(char d){
			this.dato=d;
			siguiente=null;
		}
	}
	
	private int numElem;
	private CElemento pila;
	
	public pilad(){
		numElem = 0;
	}
	
	public int size(){
		return numElem;
	}
	
	public void push(char d){		//anadir
		CElemento ch = new CElemento(d);
		if (pila != null) {
			ch.siguiente = ultimo.siguiente;
			ultimo.siguiente = nuevo;
		} else {
			ultimo = nuevo;
			ultimo.siguiente = nuevo;
		}
		tamanio++;
	}
	
	public void pop(){		//obtener
		
	}
}