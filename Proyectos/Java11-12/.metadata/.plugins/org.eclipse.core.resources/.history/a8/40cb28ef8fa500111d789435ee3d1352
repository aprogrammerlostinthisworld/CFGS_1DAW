import java.util.ArrayList;

public class test{
	
	public static boolean isIgual(colad c,pilad p){
		boolean igual = false;
		if(c.pop()==p.pop())
			igual = true;
		
		return igual;
	}

	public static void main(String[] args) {
		pilad p = new pilad();
		colad c = new colad();
		int j = 0;	
		ArrayList<Character> ch = new ArrayList<Character>();
		String cadena = "oso";

		while(j<cadena.length()){
			ch.add(cadena.charAt(j));
			if(cadena.charAt(j)!=' '){
				c.push(ch.get(j));
				p.push(ch.get(j));
			}
			j++;
		}
		System.out.println(isIgual(c,p));

		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
}