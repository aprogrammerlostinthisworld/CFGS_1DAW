package ejercicios_resueltos;

public class ejercicio5 {
	private char letra;
	ejercicio5(char l){
		letra=l;
		if(l>='a'){
			letra -='a';
			letra +='A';
		}
	}
	
	public char getLetra(){
		return letra;
	}
	
	public void printLetra(){
		System.out.println(letra);	
	}
	
	public static void main(String[] args) {
		ejercicio5 letra = new ejercicio5('p');
		letra.getLetra();
		letra.printLetra();
		ejercicio5 letra1 = new ejercicio5('g');
		letra1.getLetra();
		letra1.printLetra();
	}
}