package lista_circular;

public class colas<T> extends listac {

	listac<T> cola;
	
	public colas(){
		cola = new listac();
	}
	
	public int size(){
		return cola.tamanio();
	}
	
	public void push(T obj){				//añadir
		cola.addLast(obj);
	}
	
	public T pop(){							//obtener
		return cola.borrar();
	}
	
	public boolean colaVacia(){
		boolean res = false;
		
		return res;
	}
}
