package lista_circular;

public class test {

	public static void mostrarListac(listac<Cdatos> lista){
		int tam=lista.tamanio();
		Cdatos prueba;
		
		if(tam==0){
			System.out.println("lista vacía");
		}else{
			for(int i=0;i<tam;i++){
				prueba=(Cdatos)lista.obtener(i);
				System.out.println(i+".- "+prueba.getNombre()+" "+prueba.getNota());
			}
		}
	}
	
	public static void main(String[] args) {
		listac<Cdatos> lista = new listac<Cdatos>();
	}
}